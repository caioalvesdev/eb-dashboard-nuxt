{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.classAttributes": ["class", "ui"],
  "tailwindCSS.experimental.classRegex": [
    ["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
   "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false,
}
